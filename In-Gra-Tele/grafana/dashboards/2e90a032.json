                }
            },
            "targets": [
                {
                    "query": "\n                    from(bucket: \"datos\")\n                    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n                    |> filter(fn: (r) => r[\"topic\"] == \"Si/2e90a032/IMU\")\n                    |> filter(fn: (r) => r[\"_field\"] == \"data_accelerometer_x\" or r[\"_field\"] == \"data_accelerometer_y\" or r[\"_field\"] == \"data_accelerometer_z\")\n                    |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: true)\n                    |> yield(name: \"mean\")\n                    "
                }
            ]
